DB SETUP
CREATE EXTENSION "uuid-ossp";

-- shiva.contact definition

-- Drop table

-- DROP TABLE shiva.contact;

CREATE TABLE shiva.contact (
	id int8 NOT NULL,
	created_at timestamp NULL,
	email varchar(512) NULL,
	firstname varchar(256) NULL,
	lastname varchar(256) NULL,
	location text NULL,
	logo bytea NULL,
	notes text NULL,
	code uuid NOT NULL DEFAULT shiva.uuid_generate_v4(),
	CONSTRAINT contact_pkey PRIMARY KEY (id)
);




POST http://localhost:8080/rest/api/v1/contact
